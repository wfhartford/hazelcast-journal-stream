<configuration>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d{HH:mm:ss.SSS} %-5level - %msg%n</pattern>
    </encoder>
  </appender>

  <logger name="ca.cutterslade" level="info"/>
  <logger name="com.hazelcast.jet.impl.connector.WriteLoggerP" level="info"/>

  <root level="warn">
    <appender-ref ref="STDOUT"/>
  </root>
</configuration>
